{

  "manifest_version": 2,
  "name": "YouTube search-autofocus fix",
  "version": "1.0",

  "description": "Fixes search-autofocus on YouTube. Bug: https://bugzilla.mozilla.org/show_bug.cgi?id=1463563",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["ytsearchfix.js"]
    }
  ]

}